{
  "name": "for_front_config",
  "author": "Anton Golovin",
  "license": "ISC",
  "version": "3.0.0",
  "description": "**For Front Config** позволяет легко и быстро начать работу над проектом с использованием \"Gulp + Webpack + Jest + Lint(s)\".",
  "repository": {
    "type": "git",
    "url": "git+https://git.forforce.com/forforce-dev/init-projects.git"
  },
  "homepage": "https://git.forforce.com/forforce-dev/init-projects/tree/master/For-Twig-Config",
  "main": "config/gulpfile.babel.js",
  "scripts": {
    "start": "npm-run-all --parallel server webpack_local gulp_local",
    "server": "NODE_ENV=local webpack-dev-server --config config/webpack.config.local.js",
    "gulp_local": "NODE_ENV=local gulp --gulpfile config/gulpfile.babel.js",
    "gulp_dev": "NODE_ENV=development gulp --gulpfile config/gulpfile.babel.js",
    "gulp_stage": "NODE_ENV=stage gulp --gulpfile config/gulpfile.babel.js",
    "gulp_prod": "NODE_ENV=production gulp --gulpfile config/gulpfile.babel.js",
    "webpack_prod": "NODE_ENV=production webpack --config config/webpack.config.prod.js",
    "webpack_dev": "NODE_ENV=development webpack --config config/webpack.config.dev.js",
    "webpack_stage": "NODE_ENV=stage webpack --config config/webpack.config.prod.js",
    "webpack_local": "NODE_ENV=local webpack --config config/webpack.config.local.js --watch --progress",
    "lint:js": "jest --projects \"./config/jest/jest.eslint.config.json\"",
    "lint:scss": "jest --projects \"./config/jest/jest.stylelint.config.json\"",
    "fix:js": "prettier-eslint --write $PWD/'./private/**/*.{js,ts,jsx,tsx,vue}' $PWD/'./private/*.{js,ts}'",
    "fix:scss": "prettier-stylelint --write \"./private/**/*.scss\"",
    "fix:json": "prettier --write \"./private/**/*.json\"",
    "build_dev": "npm run gulp_dev && npm run webpack_dev",
    "build_prod": "npm run gulp_prod && npm run webpack_prod",
    "build_stage": "npm run gulp_stage && npm run webpack_stage",
    "test": "npm run lint:js && npm run lint:scss"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "cache-loader": "^1.2.0",
    "css-loader": "^0.28.11",
    "dart-sass": "^1.25.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-boundaries": "^1.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-webpack-plugin": "^2.1.0",
    "file-loader": "^2.0.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-babel": "^6.1.1",
    "gulp-filter": "^5.0.0",
    "gulp-flatten": "^0.3.1",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-svg-sprites": "^4.1.1",
    "gulp-tinypng-compress": "^1.2.1",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-pngquant": "^5.0.0",
    "jest": "^23.5.0",
    "jest-runner-eslint": "^0.10.0",
    "jest-runner-stylelint": "^1.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^2.1.4",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-stylelint": "^0.4.2",
    "raw-loader": "^4.0.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.10.1",
    "tinypng-webpack-plugin": "^2.0.0",
    "twig": "1.13.3",
    "twig-html-loader": "^0.1.8",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-glob-entry": "^2.1.1",
    "webpack-livereload-plugin": "^2.1.1",
    "webpack-merge": "^4.2.2",
    "webpack-notifier": "^1.8.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "husky": "^4.3.0"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "jquery": "^3.2.1",
    "jquery-mask-plugin": "^1.14.16",
    "jquery-validation": "^1.19.2",
    "normalize.css": "^8.0.1",
    "popper.js": "^1.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {}
}
